modules:
  icmp:
    prober: icmp
    timeout: 5s
    icmp:
      preferred_ip_protocol: "ip4"
  tcp_connect:
    prober: tcp
    timeout: 5s
  http_2xx:
    prober: http
    timeout: 5s
    http:
      valid_http_versions: ["HTTP/1.1", "HTTP/2.0"]
      valid_status_codes: [200]
      method: GET
  http_post_2xx:
    prober: http
    timeout: 5s
    http:
      valid_http_versions: ["HTTP/1.1", "HTTP/2.0"]
      valid_status_codes: [200]
      method: POST
  tcp_v4_tunnel:
    prober: tcp
    timeout: 5s
    tcp:
      preferred_ip_protocol: "ip4"
  tcp_v6_tunnel:
    prober: tcp
    timeout: 5s
    tcp:
      preferred_ip_protocol: "ip6"
